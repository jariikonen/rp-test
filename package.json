{
  "name": "@jariikonen/rp-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsdown --sourcemap --watch",
    "build": "tsdown",
    "typecheck": "tsc",
    "cleanup": "rm -rf ./dist && rm -rf ./coverage && rm -rf ./node_modules",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:watch": "vitest watch --coverage",
    "prepare": "git config core.hooksPath git-hooks"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.0.13",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "tsdown": "^0.12.9",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  }
}
